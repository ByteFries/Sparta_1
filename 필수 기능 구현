#include <iostream>

using namespace std;

void GetArray(int arr[])
{
    for (int i = 0; i < 5; i++)
    {
        cin >> arr[i];
    }
}

int Sum(int* arr)
{
    int sum = 0;

    for (int i = 0; i < 5; i++)
    {
        sum += arr[i];
    }

    return sum;
}


int main()
{ 
    int arr[5];
    GetArray(arr);
    cout << Sum(arr) << endl;
}
